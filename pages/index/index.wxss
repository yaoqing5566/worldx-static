/**index.wxss**/
.index .list{
  background-color: #fff; position: relative; padding-bottom: 20px; margin-bottom: 10px; padding-top: 20px;
}
.index .list .ico{
  width: 40px; height: 40px; border:1px solid #c9c9c9; border-radius: 10px; left: 10px; position: absolute;
}
.index .list .con{
  padding: 0 30px 0 70px;
}
.index .list .con .title{
  font-weight: bold; padding: 0px 0 15px 0;
}
.index .list .page-section-spacing{ overflow: hidden; font-size: 10px; color:rgba(0,0,0,0.9)}
.index .list .flex-item{
  width: 25%; float: left; border-right: 1px solid #c9c9c9;
}
.index .list .flex-item view{ padding-bottom: 2px}
.index .list .flex-item.a2{padding-left: 8px;width: calc(25% - 9px)}
.index .list .flex-item .t{padding: 2px 0 0 0}
.index .list .flex-item.a2 image{ width: 10px; }
.index .list .flex-item:last-child{border: none}
.index .list .progress{
  height: 12px; background-color: #f5f5f5; position: relative; margin-top: 16px; border-radius: 6px; overflow: hidden;
}
.index .list .progress image{ height: 12px; position: absolute; left: 0px; top: 0px; width: 100%; z-index: 1}
.index .list .progress .text{ font-size: 9px; color: #fff; position: absolute; top: -1px; left: 3px; z-index: 5}
.index .list .banner{
  width: 100%;height: 0px; padding-bottom: 32.2413%; position: relative; margin-top:10px; overflow: hidden;
}
.index .list .banner image{ height: 100%; position: absolute; width: 100%; left: 0px; top: 50%; transform: translateY(-50%)}
.index .list .but{margin: 15px 0 0 0; position: relative;padding: 5px 0 10px 0}
.index .list .but button{ vertical-align: middle; position: absolute; right: 0px; top: 0px;padding: 3px 15px}
.index .list .but lable{
   color: #f13f59;
}